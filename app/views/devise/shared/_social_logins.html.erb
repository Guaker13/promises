<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>

    <% link_to " sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-block btn-social btn-linkedin' do %>
    <span class="<%= "fa fa-#{OmniAuth::Utils.camelize(provider)}".downcase%>"></span>
    <% end %>
    <br />
  <% end -%>
<% end -%>
