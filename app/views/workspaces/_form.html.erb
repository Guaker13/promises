<%= simple_form_for @workspace, html: { class: 'form-box' } do |f| %>
  <%= f.input :name %>
  <%= f.input :capacity %>
  <%= f.input :location %>
  <%= f.input :square_meters %>
  <%= f.input :wifi %>
  <%= f.input :price %>
  <%= f.input :price_unit %>
  <%= f.simple_fields_for :pictures do |pictures_form| %>
    <%= pictures_form.input :photo %>
    <%= pictures_form.input :photo_cache, as: :hidden %>
  <% end -%>
  <%= f.button :submit %>
<% end -%>
